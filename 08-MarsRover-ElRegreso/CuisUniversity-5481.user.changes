!methodRemoval: MarsRoverLogTest #test04LogOnlyDirectionKnowsActualDirection stamp: 's 10/25/2022 18:11:26'!
test04LogOnlyDirectionKnowsActualDirection
	| marsRover initialPosition logger |
	
	
	initialPosition := 1@1.
	marsRover := MarsRover at: initialPosition heading: MarsRoverHeadingNorth. 
	logger :=  DirectionLogger for: marsRover .	
	marsRover addLogger: logger.
	
	marsRover process: 'r'.
	
	self assert: logger actualValue equals: 'East'.
!

!methodRemoval: MarsRoverLogTest #test03LogOnlyDirectionKnowsInitialDirection stamp: 's 10/25/2022 18:11:28'!
test03LogOnlyDirectionKnowsInitialDirection
	| marsRover initialPosition logger |
	
	initialPosition := 1@1.
	marsRover := MarsRover at: initialPosition heading: MarsRoverHeadingNorth . 
	logger :=  DirectionLogger for: marsRover .	
	
	self assert: logger actualValue equals: 'North'.
!

!methodRemoval: MarsRoverLogTest #test02LogActualPositionOneCommand stamp: 's 10/25/2022 18:11:29'!
test02LogActualPositionOneCommand
	| marsRover logger initialPosition |
	

	initialPosition := 1@1.
